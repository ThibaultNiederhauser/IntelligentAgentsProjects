<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### AUCTION AGENTS ### -->
	<agent name="auction-random">
		<set class-path="bin/"/>
		<set class-name="template.AuctionTemplate"/>
	</agent>

	<agent name="auction-numb">
		<set class-path="bin/"/>
		<set class-name = "numb.AuctionNumb"/>
		<set prob="0.8"/>
		<set lookIter="5000"/>
	</agent>

	<agent name="auction-futur">
		<set class-path="bin/"/>
		<set class-name = "futur.AuctionFutur"/>
		<set prob="0.8"/>
		<set lookIter="500"/>
	</agent>

	<agent name="auction-jelena">
		<set class-path="bin/"/>
		<set class-name = "templateJ.CentralizedAuctionAgent"/>
	</agent>
	
</agents>
