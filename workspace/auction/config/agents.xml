<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### AUCTION AGENTS ### -->
	<agent name="auction-random">
		<set class-path="bin/"/>
		<set class-name="template.AuctionTemplate"/>
	</agent>

	<agent name="auction-numb">
		<set class-path="bin/"/>
		<set class-name = "AuctionNumb"/>
		<set prob="0.8"/>
		<set lookIter="300"/>
	</agent>

	<agent name="auction-futur">
		<set class-path="bin/"/>
		<set class-name = "AuctionFutur"/>
		<set prob="0.8"/>
		<set lookIter="300"/>
		<set nodeResolution="0.01"/>
		<set nodeTrash="0.0002"/>
	</agent>

	<agent name="auction-jelena">
		<set class-path="bin/"/>
		<set class-name = "templateJ.CentralizedAuctionAgent"/>
	</agent>

	<agent name="auction-futur-simple">
		<set class-path="bin/"/>
		<set class-name = "AuctionFuturSimple"/>
		<set prob="0.8"/>
		<set lookIter="300"/>
	</agent>


	
</agents>
